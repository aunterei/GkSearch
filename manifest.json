{
  "name": "GK Search",
  "description": "An extension to search multiple video games key providers at once",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/assets/icons/icon16.png",
      "32": "/assets/icons/icon32.png",
      "48": "/assets/icons/icon48.png",
      "128": "/assets/icons/icon128.png"
    }
  },
  "icons": {
    "16": "/assets/icons/icon16.png",
    "32": "/assets/icons/icon32.png",
    "48": "/assets/icons/icon48.png",
    "128": "/assets/icons/icon128.png"
  }
}
